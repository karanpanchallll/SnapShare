{
    "version": 2,
    "builds": [
      {
        "src": "api/file.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/upload",
        "dest": "/api/file.js"
      },
      {
        "src": "/api/file/(.*)",
        "dest": "/api/file.js"
      }
    ]
  }
  
